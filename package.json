{
  "name": "fnatic-portfolio",
  "version": "0.0.1",
  "private": false,
  "author": "Jonas Raphael Schultheiss",
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
